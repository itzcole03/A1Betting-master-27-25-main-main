# Cursor AI GUI Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Application Configuration
VITE_APP_TITLE=Cursor AI GUI
VITE_APP_VERSION=1.0.0

# Debug Mode
VITE_DEBUG_MODE=false

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_FILE_UPLOAD=true

# Polling Intervals (milliseconds)
VITE_METRICS_REFRESH_INTERVAL=5000
VITE_TYPESCRIPT_REFRESH_INTERVAL=10000
VITE_AGENTS_REFRESH_INTERVAL=3000

# UI Configuration
VITE_DEFAULT_THEME=dark
VITE_SIDEBAR_DEFAULT_OPEN=true
VITE_NOTIFICATIONS_DEFAULT_ENABLED=true
